mixin sOfficial(data)
	// start sOfficial
	+b.SECTION.sOfficial.section#sOfficial&attributes(attributes)
		.container
			+e.wrap
				+e.row.row
					.col-sm-auto
						+e.img-wrap
							+e.line
							+img('img/sOfficial-logo.png')
					.col-sm
						h2 We are official PrestaShop partners
	// end sOfficial